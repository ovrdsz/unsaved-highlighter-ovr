{
  "name": "unsaved-highlighter-ovr",
  "displayName": "Unsaved Lines Highlighter",
  "description": "Highlights unsaved lines in the editor",
  "version": "0.0.1",
  "engines": {
      "vscode": "^1.80.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onStartupFinished"],
  "main": "./dist/extension.js",
  "scripts": {
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
      "@types/vscode": "^1.80.0",
      "@types/node": "^16.x",
      "typescript": "^5.1.3"
  }
}