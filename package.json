  },
  "categories": ["Other"],
  "activationEvents": ["onStartupFinished"],
  "main": "./dist/extension.js",
  "scripts": {
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "package": "vsce package"
  },
  "devDependencies": {
      "@types/vscode": "^1.80.0",
      "@types/node": "^16.x",
      "typescript": "^5.1.3",
      "@vscode/vsce": "^2.15.0"
  }
}