{
  "name": "unsaved-highlighter-ovr",
  "displayName": "Unsaved Highlighter",
  "description": "Highlights unsaved lines of code in the editor",
  "version": "0.0.1",
  "repository": {
      "type": "git",
      "url": "https://github.com/ovrdsz/unsaved-highlighter-ovr"
  },
  "engines": {
